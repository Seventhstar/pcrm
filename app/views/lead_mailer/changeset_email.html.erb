<h3>Пользователь "<%= find_version_author_name(@version) %>" изменил лид <%= link_to '#'+@lead.id.to_s, @lead %></h3>

<% puts @version.to_s %>

<p>Изменения в лиде <b><%= @lead.info  %></b>:</p>

  <%- @history.each do |version_fields| %>
  <%- version = version_fields[1] %>
	<%- version['description'].each do |descr| %>
	       <li><%= raw(descr) %></li>
                <% puts "descr: "+descr %>
        <% end %>
    </ul>
  <% end %>