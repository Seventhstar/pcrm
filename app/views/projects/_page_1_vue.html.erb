<script>
  Vue.component('v-select', VueSelect.VueSelect)
  
  let ptype = new Vue({
    el: '.prj_type',
    data: <%= fill_vue_data(@project,
     {
      model: 'project',

      complexProjectType: false,
      list_values: 'project_type city',
      lists: "project_types+simple cities"      
    }) %>,

    watch: {
      project_type(){
        console.log("this.project_type", this.project_type)
        if (this.project_type === undefined){
          this.complexProjectType = false
        } else {
          this.complexProjectType = !this.project_type.simple;
        }
      }
    }

  });

</script>