<script>
  Vue.component('v-select', VueSelect.VueSelect)

  let nav = new Vue({
    el: '.nav_city',
    data: <%= fill_vue_data(nil, {
      list_values: 'main_city',
      lists: 'cities'
    }, 'app') %>,

    watch: {
      main_city(){
        $.ajax({
          type: "POST",
          url: "/ajax/set_city",
          data: {city: this.main_city.value},
          dataType: "json",
          encode: true
        })
        delay('sortable_query({})',700)
      }
    }
   })
</script>