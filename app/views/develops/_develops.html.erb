<div class="panel-right right"> <%= will_paginate @develops %> </div>

<% cur_prj  = nil %>
  <% first_table = true %>
    
  
<% @develops.each do |develop| %>

  <% prj_name = project_name(develop.id) %>
  <% if cur_prj != prj_name %>
    <% cur_prj = prj_name %>
    <%= render "head", {:first_table => first_table, :heading => prj_name } %>
    <% first_table = false %>
  <% end %>        

  <tr class="dev_priority<%=develop.priority_id%>">
    <td ><%= develop.name %></td>
    <td class="state"><span class="check_img sw_check <%= [2,3].include?(develop.dev_status_id) ? "checked" :"" %>" developid="<%=develop.id%>" chk="coder"></span></td>
    <td class="state"><span class="check_img sw_check <%= develop.dev_status_id==3 ? "checked" :"" %>" developid="<%=develop.id%>" chk="boss"></span></td>
    <%= td_tool_icons(develop,'edit,show') %>
  </tr>
<% end %>

<% if @develops.count>0 %>
  </tbody></table></div>
<% end %>
