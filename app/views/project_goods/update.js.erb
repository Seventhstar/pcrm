<% if @prj_good.errors.present? %>
  <%= render 'errors', item: @prj_good %>
<% else %>                                  
  $('.modal').modal('hide');
  updateOptionRow("goods", 
                  <%= for_vue(@prj_good, [:currency_short, :provider_name]) %>, 
                  <%= @prj_good.id %>, false, '<%= @group %>');

  app.makeFilter();
  updAttachments(<%= for_vue(@prj_good.attachments, 'owner_name') %>, <%= @prj_good.id %>);
<% end %>
