<% if @prj_good.present? %>
  $('.modal').modal('hide');
  updateOptionRow("goods", 
                  <%= @prj_good.to_json(except: [:created_at, :updated_at], 
                                        methods: [:currency_short, :provider_name]).html_safe %>, 
                  <%= @prj_good.goodstype_id %>, true);
  app.makeFilter();
<% end %>
